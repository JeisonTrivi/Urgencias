@model Urgencias.Models.Paciente

<h1>Registro Paciente Urgencias</h1>

<form asp-action="Registro" method="post">

    <div asp-validation-summary="All"></div>

    <div class="form-group">
        <label> @Html.CheckBox("Atendido", false)
            Â¿Ha Sido Atendido antes ?</label>
    </div>

    <div class="form-group">
        <label>Nombre</label>
        <input asp-for="Nombre" class="form-control">
        <span class="text-danger" asp-validation-for="Nombre"></span>
    </div>
    <div class="form-group">
        <label>Apellido</label>
        <input asp-for="Apellido" class="form-control">
        <span class="text-danger" asp-validation-for="Apellido"></span>
    </div>
    <div class="form-group">
        <label>Genero</label>
        <input asp-for="Genero" class="form-control">
        <span class="text-danger" asp-validation-for="Genero"></span>
    </div>
    <div class="form-group">
        <label>FechaNacimiento</label>
        <input asp-for="FechaNacimiento" class="form-control"  type="datetime-local" data-val="true" value="2022-04-04">
        <span class="text-danger" asp-validation-for="FechaNacimiento"></span>
    </div>
    <div class="form-group">
        <label>Profesion</label>
        <input asp-for="Profesion" class="form-control" >
        <span class="text-danger" asp-validation-for="Profesion"></span>
    </div>
    
 <button type="submit" class="btn btn-primary table-btn">Registrar Paciente</button>
    
</form>